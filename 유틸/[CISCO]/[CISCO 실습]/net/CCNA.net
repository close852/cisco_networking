model = 2691
autostart = False
ghostios = true
sparsemem = true
mmap = False

[localhost]
port = 7200
udp = 10000
workingdir = ..\tmp\


    [[2691]]
        image = ..\ios\C2691-AD.BIN
        ram = 256
        confreg = 0x2102
        exec_area = 32
        idlemax = 100

    [[router R1]]
	console = 2001
	slot1 = NM-4T
	s1/0 = R2 s1/1
	s1/3 = FRSW 1
             #f0/0 = NIO_gen_eth:\Device\NPF_{PC Loopback}
	fa0/1 = SW1 1

    [[router R2]]
	console = 2002
	slot1 = NM-4T
	s1/0 = R3 s1/1
	s1/3 = FRSW 2
	fa0/1 = SW1 2

    [[router R3]]
	console = 2003
	slot1 = NM-4T
	s1/0 = R1 s1/1
	s1/3 = FRSW 3
	#f0/0 = NIO_gen_eth:\Device\NPF_{PC NIC}
	fa0/1 = SW1 3

    [[router R4]]
	console = 2004
	slot1 = NM-4T
	fa0/1 = R5 fa0/1

    [[router R5]]
	console = 2005
	slot1 = NM-4T

    [[ETHSW SW1]]
	1 = access 1 
	2 = access 1 
	3 = access 1 

    [[FRSW FRSW]]
	1:102 = 2:201
	1:103 = 3:301
	2:203 = 3:302